  "scripts": {
    "updateversion": "node scripts/updateversion.js --manifest ./src/manifest.webapp",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && yarn services",
    "build:analysis": "ANALYSIS=true node scripts/build.js",
    "start:analysis": "ANALYSIS=true node scripts/start.js",
    "test": "node scripts/test.js",
    "services": "node scripts/buildservices.js",
    "services:analysis": "ANALYSIS=true node scripts/buildservices.js",
    "services:speed_analysis": "ASPEED=true node scripts/buildservices.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./sdk"
  },
  "sideEffects": [
    "*.css"
  ],
  "browserslist": [
    "last 5 Chrome versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "sdk",
      "src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
